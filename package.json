{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahat/hackathon-starter.git"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve:gcloud",
    "sequelize": "./node_modules/.bin/sequelize",
    "sequelize:migrate": "./node_modules/.bin/sequelize db:migrate",
    "deploy:gcloud": "npm run build && rm -rf ./dist && gcloud app deploy",
    "serve:gcloud": "npm run build-babel && node dist/index.js",
    "build": "npm run clear && npm run build-ts && npm run build-babel",
    "serve": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\" \"npm run serve\"",
    "test": "jest --forceExit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-webpack": "webpack",
    "watch-webpack": "webpack --watch",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "copy-static-assets": "node copyStaticAssets.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "npm run clear && concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\"",
    "logs": "heroku logs --tail",
    "dev": "npm run build && nodemon dist/index.js",
    "build-babel": "./node_modules/.bin/babel build --out-dir dist --source-maps",
    "watch-babel": "./node_modules/.bin/babel-watch build/index.js",
    "watch-babel:prod": "NODE_ENV=production ./node_modules/.bin/babel-watch build/index.js",
    "watch-babel:prod:win": ".\\node_modules\\.bin\\babel-watch build\\index.js",
    "docker:mongo": "cd docker && docker-compose -f docker-compose.mongo.yml up -d",
    "docker:mongo:stop": "cd docker && docker-compose -f docker-compose.mongo.yml stop",
    "docker:pg": "cd docker && docker-compose -f docker-compose.postgresql.yml up -d",
    "docker:pg:stop": "cd docker && docker-compose -f docker-compose.postgresql.yml stop",
    "clear": "rm -rf ./build && rm -rf ./dist",
    "heroku:config:push": "heroku config:push -c -f .env -o"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "./src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "dependencies": {
    "@google-cloud/translate": "^5.0.1",
    "@types/in-app-purchase": "^1.10.0",
    "@types/sequelize": "^4.27.20",
    "ajv": "^5.2.3",
    "ajv-errors": "^1.0.0",
    "ajv-keywords": "^2.1.0",
    "apiai": "^4.0.3",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.4",
    "custom-template-generator": "^0.1.2",
    "dateformat": "^3.0.3",
    "ddos": "^0.1.25",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^4.2.1",
    "fb": "^2.0.0",
    "firebase-admin": "^5.13.1",
    "geoip-lite": "^1.3.5",
    "geolib": "^2.0.24",
    "get-video-duration": "^1.0.3",
    "guid": "0.0.12",
    "hbs": "^4.0.1",
    "in-app-purchase": "^1.10.7",
    "inquirer": "^6.1.0",
    "jsdom": "^15.1.1",
    "jsonexport": "^2.0.11",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "minipass": "2.7.0",
    "moment": "^2.18.1",
    "mongoose": "^4.12.0",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mv": "^2.1.1",
    "node-schedule": "^1.3.0",
    "nodemailer": "^5.1.1",
    "npm": "^6.9.0",
    "probe-image-size": "^4.0.0",
    "raven": "^2.6.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.38.1",
    "sequelize-replace-enum-postgres": "^1.4.0",
    "sharp": "^0.22.1",
    "socket.io": "^2.1.1",
    "stripe": "^6.12.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.16",
    "@types/body-parser": "^1.16.2",
    "@types/compression": "0.0.33",
    "@types/connect-mongo": "0.0.32",
    "@types/cors": "^2.8.1",
    "@types/del": "^3.0.1",
    "@types/dotenv": "^2.0.20",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/express-session": "0.0.32",
    "@types/fb": "0.0.22",
    "@types/geolib": "^2.0.23",
    "@types/jest": "^19.2.2",
    "@types/jquery": "^2.0.41",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.63",
    "@types/lokijs": "^1.5.2",
    "@types/md5": "^2.1.32",
    "@types/mongodb": "^2.1.43",
    "@types/mongoose": "^4.7.9",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^1.3.6",
    "@types/node": "^7.0.43",
    "@types/nodemailer": "^1.3.32",
    "@types/passport": "^0.3.3",
    "@types/pg": "^7.4.10",
    "@types/qs": "^6.5.1",
    "@types/request": "^2.0.6",
    "@types/request-promise": "^4.1.38",
    "@types/sequelize": "^4.0.79",
    "@types/socket.io": "^1.4.38",
    "@types/supertest": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-async-functions": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.4.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "pg": "^6.4.1",
    "sequelize-cli": "^2.8.0",
    "shelljs": "^0.7.7",
    "supertest": "^2.0.1",
    "ts-jest": "^19.0.8",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.0.0",
    "typescript": "3.5.1",
    "webpack": "^3.6.0"
  },
  "engines": {
    "node": "10.5.0"
  }
}
